{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/include/"
            ],
            "defines": [],
            "compilerPath": "/usr/local/i686elfcc/bin/i686-elf-gcc",
            "cStandard": "c17",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-ffreestanding",
                "-falign-jumps",
                "-falign-functions",
                "-falign-labels",
                "-falign-loops",
                "-fstrength-reduce",
                "-finline-functions",
                "-fno-builtin",
                "-nostdlib",
                "-nostartfiles",
                "-nodefaultlibs",
                "-m32",
                "-march=i686",
                "-mno-red-zone"
            ]
        }
    ],
    "version": 4
}